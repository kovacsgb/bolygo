{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: clang-5.0 build active file",
			"command": "/usr/bin/g++",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"-O0",
				"-Wall",
				"-std=c++14",
				"main.cpp",
				"shooting.cpp",
				"PlanetMaker_Base.cpp",
				"politrop.cpp",
				"-o",
				"bolygo",
				"-lm"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: /usr/bin/g++"
		}
	]
}